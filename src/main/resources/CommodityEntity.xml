<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="cn.edu.zucc.czq.graduate.entity.CommodityEntity">
        <table name="commodity" schema="deal" catalog=""/>
        <attributes>
            <id name="iid">
                <column name="iid" precision="11"/>
            </id>
            <basic name="address">
                <column name="address" nullable="false"/>
            </basic>
            <basic name="number">
                <column name="number" nullable="false" precision="11"/>
            </basic>
            <basic name="pic">
                <column name="pic" nullable="false"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
