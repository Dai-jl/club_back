<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="cn.edu.zucc.czq.graduate.entity.UserEntity">
        <table name="user" schema="deal" catalog=""/>
        <attributes>
            <id name="iid">
                <column name="iid" precision="8"/>
            </id>
            <basic name="name">
                <column name="name"/>
            </basic>
            <basic name="cclass">
                <column name="cclass" nullable="false"/>
            </basic>
            <basic name="major">
                <column name="major" nullable="false"/>
            </basic>
            <basic name="pic">
                <column name="pic" nullable="false"/>
            </basic>
            <basic name="phone">
                <column name="phone" nullable="false"/>
            </basic>
            <basic name="address">
                <column name="address" nullable="false"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
