<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="cn.edu.zucc.czq.graduate.entity.OnshowEntity">
        <table name="onshow" schema="deal" catalog=""/>
        <attributes>
            <id name="iid">
                <column name="iid" precision="11"/>
            </id>
            <basic name="name">
                <column name="name" nullable="false"/>
            </basic>
            <basic name="detail">
                <column name="detail" nullable="false"/>
            </basic>
            <basic name="pic">
                <column name="pic" nullable="false"/>
            </basic>
            <basic name="uid">
                <column name="uid" nullable="false" precision="11"/>
            </basic>
            <basic name="type">
                <column name="type" nullable="false" precision="11"/>
            </basic>
            <basic name="price">
                <column name="price" nullable="false" precision="11"/>
            </basic>
            <basic name="time">
                <column name="time" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
